#include <iostream>
#include <stdio.h>
using namespace std;

int prime(int n) 
{
    int p = 1;
    if (n == 0 || n == 1) 
        {p = 0;}
    else 
        {for (int i = 2; i <= n/2; ++i) 
            {if (n % i == 0) 
                {p = 0;
                break;}
            }
        }
    return p;
}

int main()
{   
    int num,a,b;
    cout<<"Enter a Positive Integer: ";
    cin>>num;
    a = 1; b = num-1;
    
    for (int i=1; i<=(num/2); i++)
        {if (prime(a)==1 && prime(b)==1)
            {cout<<num<<" = "<<a<<" + "<<b<<endl;}
         a = a+1;
         b = b-1;
        }
    return 0;
}
